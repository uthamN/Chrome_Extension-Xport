{
    "name": "Table to CSV Exporter",
    "version": "1.2",
    "manifest_version": 3,
    "permissions": ["scripting", "activeTab", "downloads"],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
      }
    }
  }
  